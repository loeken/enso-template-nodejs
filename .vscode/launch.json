{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Program",
            "type": "node",
            "request": "launch",
            // "skipFiles": [
            //     "<node_internals>/**"
            // ],
            "program": "${workspaceFolder}/server.js",
            "runtimeExecutable": "pnpm",
            "runtimeArgs": [
                "run-script",
                "start"
            ],
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ],
            "console": "integratedTerminal"
        }
    ]
}
